{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    ["@babel/plugin-transform-runtime"],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "src": "./src",
          "core": "./src/components",
          "theme": "./src/theme",
          "utils": "./src/utils"
        }
      }
    ]
  ]
}
